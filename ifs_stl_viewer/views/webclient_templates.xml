<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets_backend" inherit_id="web.assets_backend" name="stl_viewer assets">
            <xpath expr="." position="inside">
            	<script type="text/javascript" src="/ifs_stl_viewer/static/src/js/xeogl.js"/>
            	<script type="text/javascript" src="/ifs_stl_viewer/static/src/js/STLModel.js"/>
            	<script type="text/javascript" src="/ifs_stl_viewer/static/src/js/odoo_stl_viewer.js"/>
            </xpath>
        </template>
        
      <template id="assets_frontend" inherit_id="website.assets_frontend" name="Stl Assets">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/ifs_stl_viewer/static/src/js/xeogl.js"/>
            <script type="text/javascript" src="/ifs_stl_viewer/static/src/js/STLModel.js"/>
                        	<script type="text/javascript" src="/ifs_stl_viewer/static/src/js/website_stl_viewer.js"/>
            
        </xpath>
    </template> 
    </data>
</odoo>